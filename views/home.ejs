<%- include("partials/header") %>
<h1> Home </h1>
<p><%= startContent %></p>
<% posts.forEach(function(post){%>
<h1><%= post.title %> </h1>
<% if (post.content.length <= 100) { %>
  <p> <%= post.content%></p>
<% } else{ %>
<p> <%= post.content.substring(0,100) + " ..." %>
<a href= "/post/<%= post.title %>"   >Read more</a></p>
<% }})%>


<%- include("partials/footer") %>
